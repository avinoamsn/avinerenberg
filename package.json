{
	"name": "avinerenberg-portfolio",
	"description": "My portfolio website powered by Gatsby, react-spring, and TailwindCSS.",
	"version": "3.0.0",
	"author": "LekoArts <hello@lekoarts.de> (https://www.lekoarts.de)",
	"license": "MIT",
	"scripts": {
		"start": "npm run dev",
		"dev": "gatsby develop -o",
		"develop": "gatsby develop",
		"build": "gatsby build",
		"lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
		"lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
		"lint:ci": "npm lint --format junit -o results/eslint/result.xml",
		"deploy": "gatsby build && gh-pages -d public",
		"deploy-clean": "rm -rf public && gatsby build && gh-pages -d public"
	},
	"keywords": [
		"gatsby",
		"starter",
		"portfolio"
	],
	"dependencies": {
		"babel-plugin-styled-components": "^1.10.0",
		"gatsby": "^3.13.0",
		"gatsby-image": "^2.1.2",
		"gatsby-plugin-google-analytics": "^2.0.20",
		"gatsby-plugin-manifest": "^3.3.0",
		"gatsby-plugin-netlify": "^2.0.17",
		"gatsby-plugin-offline": "^2.1.1",
		"gatsby-plugin-react-helmet": "^3.0.12",
		"gatsby-plugin-sharp": "^3.3.0",
		"gatsby-plugin-styled-components": "^3.0.7",
		"gatsby-source-filesystem": "^2.0.38",
		"gatsby-transformer-sharp": "^3.3.0",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"styled-components": "^4.4.1",
		"react-spring": "^8.0.27",
		"tailwind.macro": "^0.5.10",
		"tailwindcss": "^0.7.4",
		"typeface-cantata-one": "^0.0.72",
		"typeface-open-sans": "^0.0.54"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.1",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.13.0",
		"prettier": "^1.18.2"
	},
	"repository": {
		"url": "https://github.com/avinoamsn/avinerenberg",
